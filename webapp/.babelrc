{
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./src/components",
          "context": "./src/context",
          "config": "./src/config",
          "containers": "./src/containers",
          "language": "./src/language",
          "utils": "./src/utils",
          "gql": "./src/gql",
          "routes": "./src/routes"
        }
      }
    ]
  ]
}
