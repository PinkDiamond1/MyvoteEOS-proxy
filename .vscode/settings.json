{
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "files.associations": {
    "iosfwd": "cpp",
    "vector": "cpp",
    "bitset": "cpp",
    "typeindex": "cpp",
    "typeinfo": "cpp",
    "__hash_table": "cpp",
    "__tree": "cpp",
    "deque": "cpp",
    "list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "__functional_03": "cpp",
    "memory_resource": "cpp",
    "functional": "cpp",
    "memory": "cpp"
  }
}
